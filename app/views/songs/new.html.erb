<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre_name%>
  <%= f.collection_select :genre_name, Genre.all, :name, :name%>
  <input name="songs[note_contents][song_notes_1]">
  <br />

  <%= f.submit %>
<% end %>
